{
    "name": "@anycrawl/ai",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "dotenv-run -r ../.. -- tsc --watch",
        "build": "dotenv-run -r ../.. -- tsc",
        "clean": "rm -rf dist",
        "typecheck": "tsc --noEmit",
        "test": "NODE_OPTIONS=--experimental-vm-modules dotenv-run -r ../.. -- jest --detectOpenHandles --forceExit",
        "test:watch": "NODE_OPTIONS=--experimental-vm-modules dotenv-run -r ../.. -- jest --watch --detectOpenHandles",
        "test:coverage": "NODE_OPTIONS=--experimental-vm-modules dotenv-run -r ../.. -- jest --coverage --detectOpenHandles --forceExit"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./utils/config.js": {
            "types": "./dist/utils/config.d.ts",
            "import": "./dist/utils/config.js",
            "default": "./dist/utils/config.js"
        },
        "./utils/helper.js": {
            "types": "./dist/utils/helper.d.ts",
            "import": "./dist/utils/helper.js",
            "default": "./dist/utils/helper.js"
        }
    },
    "dependencies": {
        "@ai-sdk/openai": "^2.0.11",
        "@ai-sdk/openai-compatible": "^1.0.6",
        "@openrouter/ai-sdk-provider": "^1.1.2",
        "@anycrawl/libs": "workspace:*",
        "ai": "^5.0.0",
        "json-schema-to-zod": "^2.6.1",
        "tiktoken": "^1.0.21",
        "zod": "^3.25.76"
    },
    "devDependencies": {
        "@anycrawl/typescript-config": "workspace:*",
        "@dotenv-run/cli": "^1.3.6",
        "@types/jest": "^29.5.14",
        "typescript": "^5.7.2",
        "jest": "^29.7.0",
        "ts-jest": "^29.3.2",
        "ts-jest-resolver": "^2.0.1"
    }
}
